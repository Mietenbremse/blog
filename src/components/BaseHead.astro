---
// Import the global.css file here so that it is included on
// all pages through the use of the <BaseHead /> component.
import "../styles/global.css";

interface Props {
	title: string;
	description: string;
	image?: string;
}

const canonicalURL = new URL(Astro.url.pathname, Astro.site);
---

<meta charset="utf-8" />
<meta name="viewport" content="width=device-width,initial-scale=1" />
<link rel="canonical" href={canonicalURL} />
<link rel="icon" type="image/svg+xml" href="/blog/favicon.svg" />
<title>mietencheck.de - Check ob du zu viel Miete zahlst</title>
<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
<link rel="manifest" href="/site.webmanifest" />
<meta name="msapplication-TileColor" content="#811C63" />
<meta name="theme-color" content="#811C63" />
<meta
	name="description"
	content="Ganz Berlin zahlt zu viel Miete! Finde heraus, ob auch du betroffen bist und senke deine Mietkosten. Prüfe jetzt kostenlos und sicher deine Miete auf mietencheck.de"
/>

<meta property="og:title" content="mietencheck.de" />
<meta property="og:type" content="website" />
<meta property="og:url" content="https://www.mietencheck.de/" />
<meta property="og:image" content="https://www.mietencheck.de/og-image.jpg" />
<meta
	property="og:description"
	content="Ganz Berlin zahlt zu viel Miete! Finde heraus, ob auch du betroffen bist und senke deine Mietkosten. Prüfe jetzt kostenlos und sicher deine Miete auf mietencheck.de"
/>
<meta property="og:site_name" content="mietencheck.de" />
<meta property="og:locale" content="de" />
<meta
	property="og:image:alt"
	content="Ganz Berlin zahlt zu viel Miete! Kannst du deine senken? Eine Initiative von Deutsche Wohnen & Co enteignen"
/>
<meta property="og:image:width" content="1200" />
<meta property="og:image:height" content="630" />
<meta name="twitter:card" content="summary_large_image" />
<meta name="twitter:title" content="mietencheck.de" />
<meta
	name="twitter:description"
	content="YGanz Berlin zahlt zu viel Miete! Finde heraus, ob auch du betroffen bist und senke deine Mietkosten. Prüfe jetzt kostenlos und sicher deine Miete auf mietencheck.de"
/>
<meta name="twitter:image" content="https://www.mietencheck.de/og-image.jpg" />
<meta
	name="twitter:image:alt"
	content="Ganz Berlin zahlt zu viel Miete! Kannst du deine senken? Eine Initiative von Deutsche Wohnen & Co enteignen"
/>
